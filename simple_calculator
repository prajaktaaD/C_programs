//calculator
#include<stdio.h>

int main(){
    char ch;
    int a,b;
    printf("Enter val of a:");
    scanf("%d",&a);
    printf("Enter val of b:");
    scanf("%d",&b);
    printf("Enter charachter(+,-,*,/:)");
    scanf("  %c",&ch);
    switch(ch)
    {
        case'+':printf("%d",a+b);
                break;
        case'-':printf("%d",a-b);
                break;
        case'*':printf("%d", a*b);
                break;
        case'/':printf("%d",a/b);
                break;
        default:printf("no other case is possible");
    }
}

  
