//entered char is vowel or not 
#include<stdio.h>
int main(){
    char ch;
    printf("enter a char:");
    scanf("%c",&ch);
    if(ch=='a'||ch=='e'||ch=='i'||ch=='o'||ch=='u'||ch=='A'||ch=='E'||ch=='I'||
    ch=='O'||ch=='U')
    printf("entered char is a vowel");
    
    else
    printf("entered char is not vowel");
}
